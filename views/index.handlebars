<form action="/workout" method="POST" role="form">
	<div class="form-group">
		<label for="distance">Distance</label>
		<select id="distance">
			<option>2000</option>
			<option>2500</option>
			<option>3000</option>
			<option>3500</option>
			<option>4000</option>
		</select>
	</div>
	<div class="form-group">
		<label for="intensity">Intensity</label>
		<select id="intensity">
			<option>Intensivt</option>
			<option>Medel</option>
			<option>Lugnt</option>
		</select>
	</div>
	<div class="form-group">
		<label for="style">Style</label>
		<select id="style">           
			<option>Snabbhet</option>
			<option>Distans</option>
		</select>
	</div>
	<button type="submit" class="btn btn-primary">Swim</button>
</form>